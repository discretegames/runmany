{
	"python.linting.enabled": true,
	"python.linting.mypyEnabled": true,
	"python.linting.mypyArgs": [
		"--strict",
		"--no-implicit-reexport"
	],
	"python.linting.pylintEnabled": false,
	"python.pythonPath": "venv\\Scripts\\python.exe",
	"cSpell.words": [
		"htmlcov",
		"infile",
		"isprime",
		"myfile",
		"nologo",
		"rawbranch",
		"rawdir",
		"rawfile",
		"runcoverage",
		"runmany",
		"runmanys",
		"runpytest",
		"runtox",
		"tdin"
	],
	"files.exclude": {
		".coverage": true,
		"desktop.ini": true,
		".pytest_cache/": true,
		".mypy_cache/": true,
		"**/__pycache__/": true,
		"**/*.egg-info/": true,
		".eggs/": true,
		".tox/": true,
		"htmlcov/": true
	}
}