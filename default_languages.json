{
	"all": "All",
	"timeout": 10.0,
	"languages": [
		{
			"name": "Python",
			"command": "python",
			"timeout": 0.1
		},
		{
			"name": "JavaScript",
			"command": "node"
		},
		{
			"name": "TypeScript",
			"command": "deno run"
		},
		{
			"name": "Java",
			"command": "java",
			"ext": ".java"
		},
		{
			"name": "Kotlin",
			"command": "kotlinc $file -include-runtime -d \"$branch.jar\" && java -jar \"$branch.jar\"",
			"ext": ".kt"
		}
	]
}
