	// JavaScript program that lists primes by checking for factors up to square root
	function isprime(n) {
		if (n === 2) {
			return true;
		}
		if (n < 2 || n % 2 === 0) {
			return false;
		}
		for (let i = 3; i <= Math.floor(Math.sqrt(n)); i += 2) {
			if (n % i === 0) {
				return false;
			}
		}
		return true;
	}

	for (let n = 0; n < parseInt(process.argv[2]); n++) {
		if (isprime(n)) {
			console.log(n);
		}
	}

