~~~| Python |~~~

def sieve(limit):
    primes = [False] * 2 + [True] * (limit - 1)
    for step in range(2, int(limit**0.5) + 1):
        if primes[step]:
            for composite in range(step**2, limit + 1, step):
                primes[composite] = False
    return primes

for n, isprime in enumerate(sieve(int(input()))):
	if isprime:
		print(n)

