	# Python program that lists primes by checking for factors up to square root
	import sys
	def isprime(n):
		if n == 2:
			return True
		if n < 2 or n % 2 == 0:
			return False
		for i in range(3, int(n**0.5) + 1, 2):
			if n % i == 0:
				return False
		return True

	for n in range(int(sys.argv[1]) + 1):
		if isprime(n):
			print(n)

